												"p_container": {
													"value": "@item().SinkContainer",
													"type": "Expression"
												},
												"p_folder": {
													"value": "@item().FolderName",
													"type": "Expression"
												}
											}
										}
									]
								},
								{
									"name": "Update_lastmodifieddate",
									"type": "Lookup",
									"dependsOn": [
										{
											"activity": "sqldbToADLS",
											"dependencyConditions": [
												"Succeeded"
											]
										}
									],

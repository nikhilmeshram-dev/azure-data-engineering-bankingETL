{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-finance-env-dev"
		},
		"ls_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls'"
		},
		"ls_azure_sql_db_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_azure_sql_db'"
		},
		"ls_azure_sqldb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_azure_sqldb'"
		},
		"ls_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stfinancedev.dfs.core.windows.net/"
		},
		"ls_api_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"ls_azure_databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0908-150321-x5zdb7ew"
		},
		"ls_azure_sql_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-finance-server.database.windows.net"
		},
		"ls_azure_sql_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sql-financeDB"
		},
		"ls_azure_sql_db_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "nikhil"
		},
		"ls_azure_sqldb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-finance-server.database.windows.net"
		},
		"ls_azure_sqldb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "rawdb"
		},
		"ls_azure_sqldb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "nikhil"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-finance-env-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_accountKey": {
						"value": "[parameters('ls_adls_accountKey')]"
					},
					"ls_azure_sql_db_password": {
						"value": "[parameters('ls_azure_sql_db_password')]"
					},
					"ls_azure_sqldb_password": {
						"value": "[parameters('ls_azure_sqldb_password')]"
					},
					"ls_adls_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_properties_typeProperties_url')]"
					},
					"ls_api_properties_typeProperties_url": {
						"value": "[parameters('ls_api_properties_typeProperties_url')]"
					},
					"ls_azure_databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_azure_databricks_properties_typeProperties_existingClusterId')]"
					},
					"ls_azure_sql_db_properties_typeProperties_server": {
						"value": "[parameters('ls_azure_sql_db_properties_typeProperties_server')]"
					},
					"ls_azure_sql_db_properties_typeProperties_database": {
						"value": "[parameters('ls_azure_sql_db_properties_typeProperties_database')]"
					},
					"ls_azure_sql_db_properties_typeProperties_userName": {
						"value": "[parameters('ls_azure_sql_db_properties_typeProperties_userName')]"
					},
					"ls_azure_sqldb_properties_typeProperties_server": {
						"value": "[parameters('ls_azure_sqldb_properties_typeProperties_server')]"
					},
					"ls_azure_sqldb_properties_typeProperties_database": {
						"value": "[parameters('ls_azure_sqldb_properties_typeProperties_database')]"
					},
					"ls_azure_sqldb_properties_typeProperties_userName": {
						"value": "[parameters('ls_azure_sqldb_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-finance-env-dev_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-finance-env-dev_ArmTemplate_0')]"
			]
		}
	]
}